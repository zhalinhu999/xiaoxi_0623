<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/default_stroke_color</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="my_itme">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:padding">15dp</item>
        <item name="android:drawableRight">@drawable/to</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/progress_default_circle_color</item>
        <item name="android:textColor">@color/btn_01</item>
    </style>

    <style name="my_itme2">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:padding">15dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/default_stroke_color</item>
        <item name="android:textColor">@color/baise</item>
    </style>

    <style name="item_content">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/sandybrown</item>
    </style>

</resources>
